@model IEnumerable<CrudAplicativoUniversidad.Models.Mdtrabajador04>

@{
    ViewData["Title"] = "Trabajador";
}

<h1>Trabajador</h1>

<p>
    <a asp-action="Create">Registrar Nuevo Trabajador</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombres@*@Html.DisplayNameFor(model => model.Nombres04)*@
            </th>
            <th>
               Apellidos @*@Html.DisplayNameFor(model => model.Apellidos04)*@
            </th>
            <th>
               Tipo @*@Html.DisplayNameFor(model => model.IdTipo03Navigation)*@
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombres04)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidos04)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipo03Navigation.Id03)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id04">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id04">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id04">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
