@model IEnumerable<CrudAplicativoUniversidad.Models.Mdalumno01>

@{
    ViewData["Title"] = "Alumnos";
}

<h1>Alumnos</h1>

<p>
    <a asp-action="Create">Registar Alumno</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
               Nombres @*@Html.DisplayNameFor(model => model.Nombres01)*@
            </th>
            <th>
               Apellidos @*@Html.DisplayNameFor(model => model.Apellidos01)*@
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombres01)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellidos01)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id01">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Id01">Ver Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.Id01">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
