    @model CrudAplicativoUniversidad.Models.Mdalumno01

@{
    ViewData["Title"] = "Registro Alumno";
}

<h1>Registro Alumno</h1>

<h4>Datos Alumno</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nombres01" class="control-label">Nombres</label>
                <input asp-for="Nombres01" class="form-control" />
                <span asp-validation-for="Nombres01" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Apellidos01" class="control-label">Apellidos</label>
                <input asp-for="Apellidos01" class="form-control" />
                <span asp-validation-for="Apellidos01" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
